{
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "@/*": ["./*"]
    },
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "strict": true,
    "noEmit": true
  },
  "include": ["src", "public/3d"],
  "exclude": ["node_modules", "dist"],
  "assetsInclude": ["**/*.gltf", "**/*.glb"]
}
